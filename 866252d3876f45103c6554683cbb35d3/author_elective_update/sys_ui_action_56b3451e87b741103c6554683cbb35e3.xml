        <script><![CDATA[function moveToApproval() {
    g_form.setValue('status', "3");
    gsftSubmit(null, g_form.getFormElement(), "status_model_move_to_approval");
}



if (typeof window == 'undefined') {
    setRedirect();

}

function setRedirect() {
    current.status = 3;
    current.update();

    createApproval();
    action.setRedirectURL(current);
}




function createApproval() {
    var approval_gr = new GlideRecord('sysapproval_approver');
    //gs.addInfoMessage(":::");
    approval_gr.initialize();
    approval_gr.state = 'requested';
    //approval_gr.setValue()'f09b2d952f370110ee07f64ef699b62c';
    //approval_gr.assignment_group.manager = '62826bf03710200044e0bfc8bcbe5df1';
    approval_gr.approver = current.assignment_group.manager; 
	approval_gr.document_id = current.sys_id;
	//Replace with approver from current record
    approval_gr.sysapproval = current.sys_id; //Current is record that requires approval
    approval_gr.insert();
}]]></script>
