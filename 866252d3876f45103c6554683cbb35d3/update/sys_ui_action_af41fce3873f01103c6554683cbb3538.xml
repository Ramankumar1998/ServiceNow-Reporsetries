}

function updateRecord() {
    var rec = new GlideRecord('sysapproval_approver');
    rec.addQuery('sysapproval', current.sys_id).addOrCondition('document_id', current.sys_id);
    rec.query();
    if (rec.next()) {
        rec.state = 'reject';
        rec.update();
    }
